@using Vidly.Models
@model IEnumerable<Vidly.Models.Book>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<table>
    @foreach (Book book in Model)
    {
        <tr><td>@Html.ActionLink(book.Name, "Details", "Books", new { id = book.Id}, null)</td></tr>
    }
</table>

